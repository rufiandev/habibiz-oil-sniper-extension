{
  "manifest_version": 3,
  "name": "Oil sniper",
  "description": "Habibiz unclaimed oil sniper",
  "version": "1.0.0",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["https://*.opensea.io/collection/thehabibiz"],
      "js": ["content-script.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["icon.png", "web3.min.js", "main.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
